		"promise-retry": "2.0.1",
		"random-seed": "0.3.0",
		"sane": "5.0.1",
		"seedrandom": "3.0.5",
		"timeout-as-promise": "1.0.0",
		"ts-node": "10.0.0",
		"twemoji-parser": "13.1.0",
